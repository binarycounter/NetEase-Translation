<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.Sherlock.ActionBar.TabBar" parent="@style/Widget">
        <item name="android:divider">?actionBarDivider</item>
        <item name="android:showDividers">middle</item>
        <item name="android:dividerPadding">12.0dip</item>
    </style>
    <style name="Sherlock.__Widget.ActivityChooserView" parent="@style/Widget">
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/abs__ab_share_pack_holo_dark</item>
        <item name="android:divider">?dividerVertical</item>
        <item name="android:minHeight">?actionBarSize</item>
        <item name="android:showDividers">middle</item>
        <item name="android:dividerPadding">6.0dip</item>
    </style>
    <style name="Sherlock.__Widget.Holo.Spinner" parent="@style/Widget">
        <item name="android:gravity">center|left</item>
        <item name="android:clickable">true</item>
        <item name="android:dropDownSelector">@drawable/abs__list_selector_holo_dark</item>
        <item name="android:popupBackground">@drawable/abs__menu_dropdown_panel_holo_dark</item>
        <item name="android:dropDownWidth">wrap_content</item>
        <item name="android:dropDownHorizontalOffset">0.0dip</item>
        <item name="android:dropDownVerticalOffset">0.0dip</item>
        <item name="android:spinnerMode">dropdown</item>
    </style>
    <style name="Widget.Sherlock.ProgressBar" parent="@android:style/Widget.ProgressBar">
        <item name="android:indeterminateDrawable">@drawable/abs__progress_medium_holo</item>
        <item name="android:animationResolution">33</item>
    </style>
    <style name="Sherlock.__Theme" parent="@android:style/Theme.Holo">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Sherlock.__Theme.Light" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="myDialogBase" parent="@style/AppTheme">
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:textColorPrimary">@color/abs__primary_text_holo_light</item>
        <item name="android:textColorPrimaryInverse">@color/abs__primary_text_holo_dark</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="vpiTabPageIndicatorStyle" parent="@style/Widget">
        <item name="android:textAppearance">@null</item>
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/inner_tab_color_selector</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/inner_tab_selector</item>
        <item name="android:divider">@drawable/tab_line</item>
        <item name="android:maxLines">1</item>
        <item name="android:height">@dimen/innerTabHeight</item>
        <item name="android:showDividers">middle</item>
        <item name="android:dividerPadding">8.0dip</item>
    </style>
    <style name="vpiMvBillBoardTabPageIndicatorStyle" parent="@style/Widget">
        <item name="android:textAppearance">@null</item>
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/inner_mv_tab_color_selector</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/inner_mv_tab_selector</item>
        <item name="android:maxLines">1</item>
        <item name="android:height">@dimen/innerMvBillTabHeight</item>
        <item name="android:width">@dimen/innerMvBillTabWitdh</item>
        <item name="android:showDividers">middle</item>
        <item name="android:dividerPadding">8.0dip</item>
    </style>
    <style name="DialogActivity" parent="@style/Theme.Sherlock.Light.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
</resources>
