<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/radioBillboardHeaderBackgroundPressedColor" />
            <stroke android:width="0.33000004dip" android:color="@color/radioBillboardHeaderBackgroundBoxColor" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/normalBackground" />
            <stroke android:width="0.33000004dip" android:color="@color/radioBillboardHeaderBackgroundBoxColor" />
        </shape>
    </item>
</selector>